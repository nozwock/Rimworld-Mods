<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <XmlExtensions.SettingsMenuDef>
        <defName>SimpleTweaks</defName> <!--Can
        be anything unique with no periods-->
        <modId>anonymous189.tweaks</modId> <!--MUST
        be the same as the one entered earlier-->
        <label>Simple Tweaks</label> <!-- This is what displays as your mod's name-->
        <settings>
            <li Class="XmlExtensions.Setting.MiddleColumn">
                <split>0.75</split>
                <settings>
                    <li Class="XmlExtensions.Setting.Gap">
                        <spacing>8</spacing>
                    </li>
                    <li Class="XmlExtensions.Setting.Text">
                        <text>Requires a restart to apply any changes.</text>
                    </li>
                    <li Class="XmlExtensions.Setting.Gap">
                        <spacing>8</spacing>
                    </li>
                    <li Class="XmlExtensions.Setting.Section">
                        <padding>12</padding>
                        <settings>
                            <li Class="XmlExtensions.Setting.Slider">
                                <key>filth_mult</key>
                                <label>Floor's Filth Multiplier: {filth_mult}</label>
                                <defaultValue>0.2</defaultValue>
                                <min>0</min>
                                <max>1</max>
                                <decimals>2</decimals>
                            </li>
                        </settings>
                    </li>
                    <li Class="XmlExtensions.Setting.Gap" />
                    <li Class="XmlExtensions.Setting.Section">
                        <padding>12</padding>
                        <settings>
                            <li Class="XmlExtensions.Setting.Checkbox">
                                <label>Adjust Steel Structure's HP and set their flamablility to 0</label>
                                <key>steel_toggle</key>
                                <defaultValue>true</defaultValue>
                            </li>
                            <li Class="XmlExtensions.Setting.ToggleableSettings">
                                <key>steel_toggle</key>
                                <caseTrue>
                                    <li Class="XmlExtensions.Setting.Slider">
                                        <key>steel_hp_mult</key>
                                        <label>Steel Structure HP Multiplier: {steel_hp_mult}</label>
                                        <defaultValue>1.5</defaultValue>
                                        <min>1</min>
                                        <max>5</max>
                                        <decimals>2</decimals>
                                    </li>
                                </caseTrue>
                            </li>
                        </settings>
                    </li>
                    <li Class="XmlExtensions.Setting.Gap" />
                    <li Class="XmlExtensions.Setting.Section">
                        <padding>12</padding>
                        <settings>
                            <li Class="XmlExtensions.Setting.Checkbox">
                                <label>Set custom range for all TVs</label>
                                <key>tv_toggle</key>
                                <defaultValue>true</defaultValue>
                            </li>
                            <li Class="XmlExtensions.Setting.ToggleableSettings">
                                <key>tv_toggle</key>
                                <caseTrue>
                                    <li Class="XmlExtensions.Setting.Slider">
                                        <key>tv_max_length_mult</key>
                                        <label>Max Length Multiplier: {tv_max_length_mult}</label>
                                        <defaultValue>2</defaultValue>
                                        <min>1</min>
                                        <max>10</max>
                                        <decimals>2</decimals>
                                    </li>
                                    <li Class="XmlExtensions.Setting.Slider">
                                        <key>tv_width_mult</key>
                                        <label>Width Multiplier: {tv_width_mult}</label>
                                        <defaultValue>2</defaultValue>
                                        <min>1</min>
                                        <max>10</max>
                                        <decimals>2</decimals>
                                    </li>
                                </caseTrue>
                            </li>
                        </settings>
                    </li>
                    <li Class="XmlExtensions.Setting.Gap" />
                    <li Class="XmlExtensions.Setting.ResetSettings">
                        <label>Reset to Defaults</label>
                    </li>
                </settings>
            </li>
        </settings>
    </XmlExtensions.SettingsMenuDef>
</Defs>